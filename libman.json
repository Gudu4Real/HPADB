{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.5.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "jquery.min.js",
        "jquery.js"
      ]
    },
    {
      "library": "jqueryui@1.12.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "jquery-ui.min.js"
      ]
    },

    {
      "library": "jQuery-formBuilder@3.5.1",
      "destination": "wwwroot/lib/formbuilder/",
      "files": [
        "form-builder.min.js",
        "form-render.min.js"
      ]
    },

    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/js/signalr",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    }
  ]
}